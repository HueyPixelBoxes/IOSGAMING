// button pressed
for i in 1...6 {
  HStack{
    Image(Int.random(1...6))
    Image(Int.random(1...6)) // dummy dice
  }
}
HStack{
    Image("\(left.val)"))
    Image("\(right.val)") // real value
}
@state var isrotating = 0


.rotaiionEffect(.degrees(isRotating))
.onAppear{
  withAnimation(.linear(duration : 2).speed (0.3).repeatForever(autoreverse: true){
  isrotating = 45
}
